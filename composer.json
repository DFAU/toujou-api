{
    "name": "dfau/toujou-api",
    "description": "TYPO3 REST API Framework",
    "type": "typo3-cms-extension",
    "license": "GPLv3",
    "replace": {
        "toujou_api": "self.version"
    },
    "require": {
        "php": ">=7.2 <7.3",
        "ext-json": "*",
        "dfau/convergence": "dev-master",
        "typo3/cms-core": "^9.5",
        "league/fractal": "~0.18",
        "league/tactician": "2.0.x-dev",
        "nikic/fast-route": "^1.3.0",
        "nikolaposa/cascader": "^1.2.0",
        "middlewares/error-handler": "^2.0.0",
        "middlewares/payload": "^2.1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "authors": [
        {
            "name": "Thomas Maroschik",
            "email": "tmaroschik@dfau.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "DFAU\\ToujouApi\\": "Classes"
        }
    }
